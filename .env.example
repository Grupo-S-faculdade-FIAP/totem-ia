# Configuração de Chaves de API
# Copie este arquivo para .env e adicione suas chaves

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face API
HF_TOKEN=your_huggingface_token_here

# Configurações do Sistema
AI_MODE=hybrid  # Opções: hybrid, openai, huggingface, simulation
AI_STRATEGY=ensemble  # Opções: ensemble, hf_first, hf_only, openai_only

# Flags de Debug
DEBUG=false
VERBOSE_ANALYSIS=true
